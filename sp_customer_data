✅ Insert Sample Data
sql
INSERT INTO dbo.customer_stg (customer_id, customer_name, email, city, updated_date)
VALUES 
    (100, 'Vamsi', 'vamsi10@gmail.com', 'Vizag', GETDATE()),
    (200, 'Krishna', 'krishna@gmail.com', 'Hyderabad', GETDATE()),
    (300, 'Vamsikrishna', 'vamsikrishna@gmail.com', 'Vizag', GETDATE());
✅ Run the Procedure
sql
EXEC dbo.customer;
✅ Update a Record in Staging
sql
UPDATE dbo.customer_stg
SET city = 'Vizag'
WHERE customer_id = 300;

EXEC dbo.customer;
